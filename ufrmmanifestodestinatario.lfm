    SQL.Strings = (
      'SELECT DISTINCT'
      'CASE when m.TIPO_EMITENTE = ''1'' THEN ''Prestador de servico de Transporte'''
      'WHEN m.TIPO_EMITENTE = ''2'' THEN ''Não prestador de serviço de Transporte'''
      'END AS EMITENTE_LETRA,'
      '--------------------------'
      'CASE WHEN m.MODALIDADE = ''1'' THEN ''Rodoviário'''
      'WHEN m.MODALIDADE = ''2'' THEN ''Aquário'''
      'END AS MODALIDADE_letra,'
      '---------------------------'
      'CASE WHEN m.TIPO_TRANSPORTADOR = ''1'' THEN ''ETC'''
      'WHEN m.TIPO_TRANSPORTADOR  = ''2'' THEN ''TAC'''
      'WHEN m.TIPO_TRANSPORTADOR = ''3'' THEN ''CTC'''
      'END AS TRANSPORTADOR_LETRA,'
      '---------------------------'
      'CASE WHEN m.FORMA_EMISSAO = ''1'' THEN ''Normal'''
      'WHEN m.FORMA_EMISSAO = ''2'' THEN ''Contigência'''
      'END AS EMISSAO_LETRA,'
      '---------------------------'
      'CASE WHEN mv.MDV_RODADO = ''1'' THEN ''Não Aplicável'''
      'WHEN mv.MDV_RODADO = ''2'' THEN ''Truck'''
      'WHEN mv.MDV_RODADO = ''3'' THEN ''Toco'''
      'WHEN mv.MDV_RODADO = ''4'' THEN ''Cavalo Mecânico'''
      'WHEN mv.MDV_RODADO = ''5'' THEN ''VAN'''
      'WHEN mv.MDV_RODADO = ''6'' THEN ''Utilitários'''
      'WHEN mv.MDV_RODADO = ''7'' THEN ''Outros'''
      'END AS RODADO_LETRA,'
      '-------------------------'
      'CASE WHEN mv.MDV_CARROCERIA = ''1'' THEN ''Não Aplicável'''
      'WHEN mv.MDV_CARROCERIA = ''2'' THEN ''Aberta'''
      'WHEN mv.MDV_CARROCERIA = ''3'' THEN ''Fechada/Baú'''
      'WHEN mv.MDV_CARROCERIA = ''4'' THEN ''Granelera'''
      'WHEN mv.MDV_CARROCERIA = ''5'' THEN ''Porta-Contêinie'''
      'WHEN mv.MDV_CARROCERIA = ''6'' THEN ''Sider'''
      'END AS CARROCERIA_LETRA,'
      '-------------------------'
      'm.serie_doc,'
      'm.numero_doc,'
      'm.data_emissao,'
      'm.data_viagem,'
      'm.tipo_emitente,'
      'm.modalidade,'
      'm.hora_emissao,'
      'm.hora_viagem,'
      'm.tipo_transportador,'
      'm.forma_emissao,'
      'm.uf_carregamento,'
      'm.uf_descarregamento,'
      'm.nf_empresa,'
      'm.status_mdfe,'
      'm.tot_mercadorias,'
      'm.tot_cte as QuantidadeCTe,'
      'm.tot_nfe as QuantidadeNFe,'
      'm.tot_nf1_nf1a as QuantidadeNF11A,'
      'm.tot_peso as pesoBrutoCargaMercadoria,'
      'm.und_medida,'
      'm.chave_mdfe,'
      'm.protocolo_mdfe,'
      '----'
      'mv.mdv_placa,'
      'mv.mdv_uf,'
      'mv.mdv_tara,'
      'mv.mdv_rodado,'
      'mv.mdv_capacidade_kg,'
      'mv.mdv_tipo,'
      'mv.mdv_capacidade_m3,'
      '-----'
      'mv.mdv_rntrc,'
      'mv.mdv_tipo,'
      'mv.mdv_cod_interno,'
      'mv.mdv_prop_uf,'
      'mv.mdv_prop_cnpj_cpf,'
      'mv.mdv_prop_insc_estadual,'
      'mv.mdv_prop_nome,'
      '------'
      'mld.mdd_chave,'
      'mld.mdd_seg_cod_barra,'
      'mld.mdd_peso,'
      'mld.mdd_valor,'
      '------'
      'mc.nome_condutor,'
      'mc.cpf_condutor,'
      '------'
      'mlc.mdc_cidade,'
      'mlc.mdc_cidade_ibge,'
      'mlc.mdc_cidade as carregamento,'
      'mlcu.mdc_cidade as descarregamento'
      'from mdfe m'
      'inner join mdfe_veiculos mv on  m.mdfe_id = mv.mdfe_id'
      'inner join mdfe_localcarga mlc on  m.mdfe_id = mlc.mdfe_id'
      'inner join mdfe_localcarga mlcu on  m.mdfe_id = mlcu.mdfe_id'
      'inner join mdfe_condutores mc on mc.mdfe_id = m.mdfe_id'
      'inner join mdfe_localcarga_docs mld on mld.mdfe_id = m.mdfe_id'
      'where 1 = 1'
      '--and mlcu.mdc_tipo = ''D'''
      '--and mlc.mdc_tipo = ''C'''
    )
