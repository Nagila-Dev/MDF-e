    SQL.Strings = (
      'SELECT'
      ''
      'CASE'
      'WHEN m.TIPO_EMITENTE = ''1'' THEN ''Prestador de servico de Transporte'''
      'WHEN m.TIPO_EMITENTE = ''2'' THEN ''Não prestador de serviço de Transporte'''
      'END AS EMITENTE_LETRA,'
      ''
      'CASE'
      'WHEN m.MODALIDADE = ''1'' THEN ''Rodoviário'''
      'WHEN m.MODALIDADE = ''2'' THEN ''Aquário'''
      'END AS MODALIDADE_letra,'
      ''
      'CASE'
      'WHEN m.TIPO_TRANSPORTADOR = ''1'' THEN ''ETC'''
      'WHEN m.TIPO_TRANSPORTADOR = ''2'' THEN ''TAC'''
      'WHEN m.TIPO_TRANSPORTADOR = ''3'' THEN ''CTC'''
      'END AS TRANSPORTADOR_LETRA,'
      ''
      'CASE'
      'WHEN m.FORMA_EMISSAO = ''1'' THEN ''Normal'''
      'WHEN m.FORMA_EMISSAO = ''2'' THEN ''Contigência'''
      'END AS EMISSAO_LETRA,'
      ''
      'MDFE_ID, NF_EMPRESA, DATA_EMISSAO, SERIE_DOC, FORMA_EMISSAO, TIPO_EMITENTE,'
      'MODALIDADE, NUMERO_DOC, HORA_EMISSAO, DATA_VIAGEM, HORA_VIAGEM,'
      'MODELO_DOC, STATUS_MDFE, OBS,'
      ''
      'UF_CARREGAMENTO, UF_DESCARREGAMENTO, TIPO_TRANSPORTADOR,'
      ''
      'TOT_CTE, TOT_NFE, TOT_NF1_NF1A, TOT_MERCADORIAS, TOT_PESO, UND_MEDIDA'
      ''
      'FROM MDFE m'
      'where MDFE_ID = :MDFE_ID'
    )
